{
	"name": "efinanceira",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "bun src/server.ts",
		"dev": "bun --watch src/server.ts",
		"lint": "bunx --bun biome check --write",
		"db:generate": "bun run --bun drizzle-kit generate",
		"db:migrate": "bun run --bun drizzle-kit migrate",
		"db:push": "bun run --bun drizzle-kit push",
		"db:studio": "bun run --bun drizzle-kit studio",
		"swagger:build": "cd node_modules/@elysiajs/swagger && bun install && bun run build || true && rm -rf node_modules && cd ../../..",
		"prepare": "husky"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.3.3",
		"@elysiajs/eden": "^1.3.2",
		"@elysiajs/server-timing": "^1.3.0",
		"@elysiajs/swagger": "git@github.com:gsbenevides2/elysia-swagger.git",
		"@grotto/logysia": "^0.1.6",
		"@hookform/resolvers": "^4.1.0",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-select": "^2.1.6",
		"@radix-ui/react-slot": "^1.1.2",
		"@tailwindcss/postcss": "^4.1.11",
		"autoprefixer": "^10.4.21",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cssnano": "^7.1.0",
		"drizzle-kit": "^0.31.1",
		"drizzle-orm": "^0.44.2",
		"elysia": "latest",
		"jose": "^6.0.12",
		"lucide-react": "^0.475.0",
		"postgres": "^3.4.7",
		"react": "^19",
		"react-dom": "^19",
		"react-hook-form": "^7.54.2",
		"react-router": "^7.7.1",
		"tailwind-merge": "^3.0.1",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/tailwindcss": "^3.1.0",
		"bun-types": "latest",
		"husky": "^9.1.7"
	}
}
